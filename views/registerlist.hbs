{{> nav2}}
<div class="container-fluid mt-5" style="width: 90%;">
        <table id="reglist" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Email Address</th>
                    <th>Contact Number</th>
                    <th>Desired Unit Number</th>
                    <th>Scheduled day of checking</th>
                    <th>Ideal day of payment</th>
                    <th>Status</th>
                    
                </tr>
            </thead>
            <tbody>
            {{#each reglist}}
             <tr>
                <td>{{Name}}</td>
                <td>{{email}}</td>
                <td>{{contact}}</td>
                <td>{{UnitNo}}</td>
                <td>{{DayChecking}}</td>
                <td>{{iday}}</td>
                 <td>{{status}}</td>
            </tr>
{{/each}}
            </tbody>
            </table>
          </div>
<div id= "tenantmodal"class="modal" role="dialog" role = "document">
    <div class="modal-dialog">
<div class = "modal-content">
    <div class="modal-header">
        <h4 class = "modal-title text-center">Tenant Information</h4>
    </div>
    <div class="modal-body">
        <form role="form" method="POST" action="">
        <div class="form-group">
        <label>Name</label>
        <div>
        <input type="text" id="editname" class= "form-control input-lg">
        </div>
        </div>
        <div class="form-group">
        <label>Day of payment</label>
        <div>
        <input type="text" id="editpay" class= "form-control input-lg">
        </div>
        </div>
        <div class="form-group">
        <label>Unit Number</label>
        <div>
        <input type="number" id="editunit" class= "form-control input-lg"> 
        </div>
        </div>
        <div class="form-group">
        <label>Rent</label>
        <div>
        <input type="text" id="editmont" class= "form-control input-lg">
        </div>
        </div>
        <div class="form-group">
        <label>Months Missed</label>
        <div>
        <input type="text" id="editmos" class= "form-control input-lg">
        </div>
        </div>
         <div class="form-group">
        <label>No. of Family Members</label>
        <div>
        <input type="text" id="editfam" class= "form-control input-lg">
        </div>
        </div>
           <div class="form-group">
          <button type="submit" class="btn btn-success btn-block">Submit Edit</button>
                    </div>
    </form>
    </div>
    </div>
    </div>
</div>